{
  "name": "typescript-api",
  "version": "1.0.0",
  "description": "Nodejs TypeScript API",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "start": "yarn build && node dist/src/index.js",
    "start:dev": "ts-node-dev 'src/index.ts'",
    "lint": "eslint ./src ./test --ext .ts",
    "lint:fix": "eslint ./src ./test --ext .ts --fix",
    "test:functional": "jest --projects ./test --runInBand"
  },
  "repository": {
    "type": "git",
    "url": "git+@https://github.com/codekoller/node-typescript-api.git"
  },
  "bugs": {
    "url": "https://github.com/codekoller/node-typescript-api/issues"
  },
  "homepage": "https://github.com/codekoller/node-typescript-api#readme",
  "keywords": [
    "nodejs",
    "typescript",
    "api"
  ],
  "author": "William Koller",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "27.0.3",
    "@types/module-alias": "^2.0.1",
    "@types/node": "16",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "3.2.0",
    "@typescript-eslint/parser": "3.2.0",
    "eslint": "7.2.0",
    "jest": "27.4.5",
    "supertest": "^6.3.3",
    "ts-jest": "27.1.2",
    "ts-node-dev": "1.0.0-pre.47",
    "typescript": "3.8.3"
  },
  "dependencies": {
    "module-alias": "^2.2.2"
  }
}
